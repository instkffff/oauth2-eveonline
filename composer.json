  "keywords": [
    "oauth",
    "oauth2",
    "authorization",
    "authentication",
    "idp",
    "identity",
    "sso",
    "single sign on",
    "ccp",
    "crest",
    "eve",
    "eve online",
    "space pixels",
    "dank memes"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Rob Bast",
    "email": "rob.bast@gmail.com"
  }],
  "require": {
    "php": "^7.0.0",
    "league/oauth2-client": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Alcohol\\OAuth2\\Client\\Provider\\": ["src"]
    }
